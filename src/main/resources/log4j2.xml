<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <Appenders>
        <Console name="console">
            <PatternLayout pattern=" [%d{yyyy-MM-dd HH:mm:ss.SSS}] [%-5p] [%m]%n" />
        </Console>
    </Appenders>

    <Loggers>
        <Root additivity="false" level="info">
            <AppenderRef ref="console"/>
        </Root>
    </Loggers>


</configuration>